<div class="modal fade" id="add-model" tabindex="-1" role="dialog" aria-labelledby="add-model-label" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="add-model-label">Tạo mới tài khoản</h5>
                <button type="button" localize-content class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form asp-action="CreateByFile" enctype="multipart/form-data">
                    <div class="file-upload-wrapper w-100">
                        <label class="control-label font-weight-bold text-primary">Chọn hoặc thả file vào vùng dưới</label>
                        <input id="input-b7" name="inputb7" data-browse-on-zone-click="true" type="file">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" localize-content class="btn btn-secondary" data-dismiss="modal">Thoát</button>
                <button type="button" localize-content class="btn btn-primary" data-save="modal">Tạo</button>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        $("#input-b7").fileinput({
            showUpload: false,
            language: "vi",
            dropZoneEnabled: true,
            rtl: true,
            allowedFileExtensions: ["xls", "xlsx"]
            //uploadUrl: '/site/file-upload-single'
        });
    });
</script>

@await Html.PartialAsync("_ValidationScriptsPartial")